{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "final_space": true,
  "version": 3,
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "session",
          "style": "plain",
          "background": "#000000",
          "foreground": "#00ff00",
          "template": "{{ if .SSHSession }}\udb82\udfc8 {{ else }}\uee15 {{ end }}{{ .UserName }} "
        },
        {
          "type": "session",
          "style": "plain",
          "background": "#000000",
          "foreground": "#00ff00",
          "template": "{{ if .SSHSession }}\udb82\udd09 {{ else }}\uf197 {{ end }}{{ .HostName }} "
        },
        {
          "type": "path",
          "style": "powerline",
          "powerline_symbol": "\uE0B0",
          "foreground": "#00ff00",
          "background": "#000000",
          "properties": {
            "style": "folder"
          }
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "type": "time",
          "style": "plain",
          "foreground": "#00ff00",
          "template": "\ue641 {{ now | date .Format }}",
          "properties": {
            "time_format": "3:04 PM (1/_2)"
          }
        }
      ]
    }
  ]
}
