{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "final_space": true,
  "version": 3,
  "palette": {
    "dark-grey": "#262626",
    "dark-blue": "#00005f",
    "brite-blue": "#00d7ff",
    "lite-grey": "#dadada",
    "orange": "#ffaf00",
    "yellow": "#d7ff00"
  },
  "secondary_prompt": {
    "foreground": "p:lite-grey",
    "template": " <p:dark-grey>\ue0b3</>\udb80\udec7 "
  },
  "transient_prompt": {
    "foreground": "p:lite-grey",
    "background": "p:dark-grey",
    "template": "<p:dark-grey,transparent>\ue0b3</> \udb81\udd0c <p:orange>{{ .UserName }}</>\ue0bb\ue0bd<p:brite-blue>{{ .HostName }}</> <p:dark-grey,transparent>\ue0b0</><p:dark-grey,transparent>\u2500\ue0b3</><,transparent>\udb80\udec7 </>"
  },
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "session",
          "style": "diamond",
          "leading_diamond": "\u256d\u2500\ue0b3<p:lite-grey> \udb81\udd0c\udb81\udd0c </>",
          "background": "p:dark-grey",
          "foreground": "p:lite-grey",
          "template": "{{ if .SSHSession }}<p:brite-blue>\udb84\udf83</> STN {{ else }}<p:brite-blue>\uf197</>  SES {{ end }}"
        },
        {
          "type": "session",
          "style": "diamond",
          "trailing_diamond": "<p:lite-grey> \udb81\udd0c\udb81\udd0c </>\ue0b1",
          "background": "p:dark-grey",
          "template": "<p:brite-blue>{{ .HostName }}</> <p:lite-grey>\udb81\udd0c</> <p:lite-grey>OPERATED BY </><p:orange>{{ .UserName }}</> <p:yellow>\uee15</>"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "type": "time",
          "style": "plain",
          "foreground": "p:yellow",
          "template": "T={{ now | date .Format }}",
          "properties": {
            "time_format": "3:04PM"
          }
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "type": "text",
          "style": "diamond",
          "leading_diamond": "\u251c\u2500\u2500\ue0b2",
          "foreground": "p:yellow",
          "background": "p:dark-grey",
          "template": "<p:lite-grey>\udb81\udd0c</> \udb84\udc80 \ueb70"
        },
        {
          "type": "path",
          "style": "diamond",
          "foreground": "p:brite-blue",
          "background": "p:dark-grey",
          "trailing_diamond": "\ue0b0",
          "properties": {
            "style": "agnoster",
            "home_icon": "\udb80\udde7 ",
            "folder_icon": "\uf115 ",
            "folder_separator_icon": "<#e0e0e0>\uf061 </>"
          }
        },
        {
          "type": "python",
          "style": "diamond",
          "foreground": "p:brite-blue",
          "background": "p:dark-grey",
          "trailing_diamond": "\uE0B0",
          "properties": {
            "display_mode": "environment"
           },
          "template": "<p:orange>\uE235</> {{ .Venv }}"
        },
        {
          "type": "git",
          "style": "diamond",
          "trailing_diamond": "\ue0b0",
          "foreground": "p:dark-grey",
          "foreground_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#00005f{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#00005f{{ end }}",
            "{{ if gt .Ahead 0 }}#00005f{{ end }}",
            "{{ if gt .Behind 0 }}#00005f{{ end }}"
          ],
          "background": "p:lite-grey",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#d9ff00{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#d9ff00{{ end }}",
            "{{ if gt .Ahead 0 }}#00d7ff{{ end }}",
            "{{ if gt .Behind 0 }}#00d7ff{{ end }}"
          ],
          "template": " {{ .UpstreamIcon }} {{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }}  {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }}  {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }}  {{ .StashCount }}{{ end }}",
          "properties": {
            "fetch_status": true,
            "fetch_upstream_icon": true,
            "untracked_modes": {
              "/Users/user/Projects/oh-my-posh/": "no"
            },
            "source": "cli",
            "mapped_branches": {
              "feat/*": "\udb81\ude91 ",
              "bug/*": "\udb80\udce4 "
            }
          }
        }
      ]
    },
    {
      "type": "prompt",
      "newline": true,
      "alignment": "left",
      "segments": [
        {
          "type": "status",
          "style": "plain",
          "properties": {
            "always_enabled": true
          },
          "foreground": "p:lite-grey",
          "foreground_templates": [
            "{{ if gt .Code 0 }}#ff0000{{ end }}"
          ],
          "template": "<p:dark-grey>\u2570\ue0b3</>\udb80\udec7 "
        }
      ]
    }
  ]
}
