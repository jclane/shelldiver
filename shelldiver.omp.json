{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "final_space": true,
  "version": 3,
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "session",
          "style": "plain",
          "background": "#00005f",
          "foreground": "#d7ff00",
          "template": "{{ if .SSHSession }}\udb82\udfc8 {{ else }}\uee15 {{ end }}"
        },
        {
          "type": "session",
          "style": "plain",
          "background": "#00005f",
          "foreground": "#00d7ff",
          "template": "{{ .UserName }} "
        },
        {
          "type": "session",
          "style": "plain",
          "background": "#00005f",
          "foreground": "#d7ff00",
          "template": "{{ if .SSHSession }}\udb82\udd09 {{ else }}\uf197 {{ end }}"
        },
        {
          "type": "session",
          "style": "plain",
          "background": "#00005f",
          "foreground": "#00d7ff",
          "template": "{{ .HostName }}"
        }
      ]
    },
      {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "type": "time",
          "style": "plain",
          "foreground": "#d7ff00",
          "template": "\ue641 {{ now | date .Format }}",
          "properties": {
            "time_format": "3:04 PM (1/_2)"
          }
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "type": "text",
          "style": "plain",
          "foreground": "#d7ff00",
          "background": "#00005f",
          "template": "\udb84\udc80 \ueb70"
        },
        {
          "type": "path",
          "style": "diamond",
          "foreground": "#00d7ff",
          "background": "#00005f",
          "trailing_diamond": "\ue0b0",
          "properties": {
            "style": "agnoster",
            "home_icon": "\udb80\udde7 ",
            "folder_icon": "\uf115 ",
            "folder_separator_icon": "<#e0e0e0>\uf061 </>"
          }
        },
        {
          "type": "git",
          "style": "diamond",
          "trailing_diamond": "\ue0b0",
          "foreground": "00005f",
          "foreground_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#00005f{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#00005f{{ end }}",
            "{{ if gt .Ahead 0 }}#00005f{{ end }}",
            "{{ if gt .Behind 0 }}#00005f{{ end }}"
          ],
          "background": "00d7ff",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#d9ff00{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#d9ff00{{ end }}",
            "{{ if gt .Ahead 0 }}#00d7ff{{ end }}",
            "{{ if gt .Behind 0 }}#00d7ff{{ end }}"
          ],
          "template": " {{ .UpstreamIcon }} {{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }}  {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }}  {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }}  {{ .StashCount }}{{ end }}",
          "properties": {
            "fetch_status": true,
            "fetch_upstream_icon": true,
            "untracked_modes": {
              "/Users/user/Projects/oh-my-posh/": "no"
            },
            "source": "cli",
            "mapped_branches": {
              "feat/*": "\udb81\ude91 ",
              "bug/*": "\udb80\udce4 "
            }
          }
        }
      ]
    },
    {
      "type": "prompt",
      "newline": true,
      "alignment": "left",
      "segments": [
        {
          "type": "status",
          "style": "plain",
          "properties": {
            "always_enabled": true
          },
          "foreground": "#00d7ff",
          "foreground_templates": [
            "{{ if gt .Code 0 }}#ff0000{{ end }}"
          ],
          "template": "\udb80\udec7 "
        }
      ]
    }
  ]
}
